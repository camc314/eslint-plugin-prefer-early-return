{
    "name": "eslint-plugin-prefer-early-return",
    "version": "1.0.0",
    "description": "Oxlint rule to prefer early returns over deeply nested if-else chains",
    "main": "dist/index.mjs",
    "type": "module",
    "scripts": {
        "build": "tsdown",
        "test": "vitest run",
        "test:watch": "vitest",
